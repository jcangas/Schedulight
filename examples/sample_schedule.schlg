

# DSL sample

hours_per_day 3

subjects 'math', 'biology', 'computers', 'philosofy'

teacher('Jhon') {

  gives 'math', 'computers'

}


teacher('Mary') {

   gives 'idiom', 'philosofy'

}


teacher('Bob') {

    gives 'history'

}


grade('First') {

    workload {
        history 3
        math   5
        biology  4
        computers 4
        philosofy 3
    }
}

classroom("A") {
    grade 'First'
}

